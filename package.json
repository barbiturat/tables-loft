    "@types/body-parser": "^0.0.33",
    "@types/classnames": "^0.0.32",
    "@types/express": "^4.0.34",
    "@types/history": "2.0.41",
    "@types/query-string": "^3.0.31",
    "@types/ramda": "^0.0.3",
    "@types/react": "^15.0.4",
