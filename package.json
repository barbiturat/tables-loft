{
  "name": "boilerplate_1",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public/resources",
    "build:dev": "webpack --config ./configs/webpack.config.js",
    "build:dev:watch": "webpack --watch --config ./configs/webpack.config.js",
    "build:prod": "webpack -p --bail --config configs/webpack.config.js",
    "build-dev": "npm run clean && npm run build:dev",
    "build-prod": "npm run clean && npm run build:prod",
    "dev-server": "webpack-dev-server --config ./configs/webpack.config.js",
    "start": "npm run clean && npm run build:dev:watch & npm run dev-server",
    "build-fake-server": "rimraf fake-server/out && tsc --project ./fake-server/tsconfig.json",
    "build-fake-server-watch": "rimraf fake-server/out && tsc --watch --project ./fake-server/tsconfig.json",
    "build-and-run-fake-server": "npm run build-fake-server && node ./fake-server/out/fake-server",
    "run-fake-server": "node ./fake-server/out/fake-server",
    "postinstall": "typings install",
    "test": "karma start configs/karma.conf.js",
    "test:cov": "karma start configs/karma.conf.js --single-run && remap-istanbul -i coverage/coverage-raw.json -o coverage/coverage.txt -t lcovonly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gdekefir@bitbucket.org/gdekefir/boilerplate_1.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/gdekefir/boilerplate_1#readme",
  "appSettings": {
    "devServerPort": 8080,
    "apiServerPort": 8081
  },
