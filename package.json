    "@types/validator": "^5.7.35",
    "autoprefixer": "^6.7.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "express": "^4.14.0",
    "html-webpack-plugin": "^2.26.0",
    "http-auth": "^3.1.1",
    "istanbul-instrumenter-loader": "^1.1.0",
    "karma": "^1.3.0",
