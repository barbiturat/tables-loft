    "react-paginate": "4.1.0",
    "react-redux": "4.4.6",
    "react-redux-form": "1.4.1",
    "react-router": "2.8.1",
    "redux": "3.6.0",
    "redux-mock-store": "1.2.1",
    "redux-observable": "0.12.2",
    "redux-router": "2.1.2",
    "remap-istanbul": "0.7.0",
    "reselect": "2.5.4",
    "rimraf": "2.5.4",
