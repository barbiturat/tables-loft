    "react": "^15.4.2",
    "react-addons-create-fragment": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-modal": "^1.6.4",
    "react-paginate": "^4.1.0",
    "react-redux": "^5.0.2",
    "react-redux-form": "^1.5.3",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
