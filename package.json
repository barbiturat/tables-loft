{
  "name": "boss-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "6.9.4"
  },
  "scripts": {
    "clean": "rimraf public/resources",
    "build:dev": "webpack --config ./configs/webpack.config.js",
    "build:dev:watch": "webpack --watch --config ./configs/webpack.config.js",
    "build:prod": "webpack -p --bail --config configs/webpack.config.js",
    "build-dev": "npm run clean && npm run build:dev",
    "build-dev-watch": "npm run clean && npm run build:dev:watch",
    "build-prod": "npm run clean && npm run build:prod",
    "build-server": "rimraf js-server/out && tsc --project ./js-server/tsconfig.json",
    "build-fake-api-server": "rimraf fake-api-server/out && tsc --project ./fake-api-server/tsconfig.json",
    "run-fake-api-server": "node ./fake-api-server/out/fake-api-server",
    "build-and-run-server": "npm run build-server && node ./js-server/out/js-server",
    "build-and-run-fake-api-server": "npm run build-fake-api-server && npm run run-fake-api-server",
    "start-dev": "npm run clean && npm run build:dev:watch & npm run build-and-run-server",
    "run-server": "node ./js-server/out/js-server",
    "test": "karma start configs/karma.conf.js",
    "test:cov": "karma start configs/karma.conf.js --single-run && remap-istanbul -i coverage/coverage-raw.json -o coverage/coverage.txt -t lcovonly",
    "start": "node ./js-server/out/js-server",
    "start-locally": "npm run build-dev & npm run build-and-run-fake-api-server & npm run build-and-run-server",
    "postinstall": "typings install && npm rebuild node-sass && npm run build-prod && npm run build-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gdekefir@bitbucket.org/gdekefir/boilerplate_1.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/gdekefir/boilerplate_1#readme",
  "browserslist": [
    "> 1%"
  ],
  "appSettings": {
    "API_KEY": 1111111,
    "API_HOST": "http://localhost",
    "API_PORT": 8081,
    "ASSETS_PORT": 5000,
    "BASIC_AUTH_USERNAME": "name",
    "BASIC_AUTH_PASSWORD": "password"
  },
